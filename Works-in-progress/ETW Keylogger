Logman create trace –n usbtrace –o “C:\windows\tracing\trace.etl” –nb 128 640 –bs 128
Logman update trace –n usbtrace –p Microsoft-Windows-USB-USBPort
Logman start -n usbtrace
Logman stop -n usbtrace

$log=get-winevent –path C:\tracing\trace.etl –oldest | where {$_.messgae –match “Data”}

Foreach ($L in $log) {write-host (‘{0:x}’ ($L.properties.value[5]))}
